tVNode(\"p\", null, \"给作者打赏\", -1 /* HOISTED */));\nconst _hoisted_21 = /*#__PURE__*/_withScopeId(() => /*#__PURE__*/_createElementVNode(\"p\", null, \"个渔币\", -1 /* HOISTED */));\nexport function render(_ctx, _cache, $props, $setup, $data, $options) {\n  const _component_el_button = _resolveComponent(\"el-button\");\n  const _component_el_input = _resolveComponent(\"el-input\");\n  const _component_el_dialog = _resolveComponent(\"el-dialog\");\n  return _openBlock(), _createElementBlock(_Fragment, null, [_createCommentVNode(\"用户头像和关注按钮  \"), _createElementVNode(\"div\", _hoisted_1, [_createElementVNode(\"div\", _hoisted_2, [_createElementVNode(\"div\", _hoisted_3, [_createElementVNode(\"img\", {\n    src: $setup.userAvatar,\n    alt: \"\",\n    class: \"navbar-avatar\",\n    onClick: $setup.navigateToUserProfile\n  }, null, 8 /* PROPS */, _hoisted_4)]), _createElementVNode(\"div\", _hoisted_5, [_createElementVNode(\"span\", _hoisted_6, _toDisplayString($setup.user.username), 1 /* TEXT */), $setup.user.verified === 1 ? (_openBlock(), _createElementBlock(\"img\", _hoisted_7)) : _createCommentVNode(\"v-if\", true)]), _createElementVNode(\"div\", _hoisted_8, [$setup.focusStatus ? (_openBlock(), _createBlock(_component_el_button, {\n    key: 0,\n    type: \"primary\",\n    round: \"\",\n    onClick: $setup.followUser\n  }, {\n    default: _withCtx(() => [_createTextVNode(\"已关注\")]),\n    _: 1 /* STABLE */\n  })) : (_openBlock(), _createBlock(_component_el_button, {\n    key: 1,\n    type: \"primary\",\n    round: \"\",\n    onClick: $setup.followUser\n  }, {\n    default: _withCtx(() => [_createTextVNode(\"+关注\")]),\n    _: 1 /* STABLE */\n  }))])]), _createElementVNode(\"div\", _hoisted_9, [_createCommentVNode(\" 点赞盒子 \"), _createElementVNode(\"div\", _hoisted_10, [_createElementVNode(\"img\", {\n    class: \"like-pic\",\n    src: $setup.likeStatus === 0 ? $setup.unlikedImage : $setup.likedImage,\n    onClick: $setup.toggleLike\n  }, null, 8 /* PROPS */, _hoisted_11), _createElementVNode(\"span\", _hoisted_12, _toDisplayString($setup.articleData.likeCount), 1 /* TEXT */)]), _createCommentVNode(\" 收藏盒子 \"), _createElementVNode(\"div\", _hoisted_13, [_createElementVNode(\"img\", {\n    class: \"like-pic\",\n    src: $setup.favoriteStatus === 0 ? $setup.unfavoritedImage : $setup.favoritedImage,\n    onClick: _cache[0] || (_cache[0] = $event => $setup.dialogFormVisible = true)\n  }, null, 8 /* PROPS */, _hoisted_14), _createElementVNode(\"span\", _hoisted_15, _toDisplayString($setup.articleData.favoriteCount), 1 /* TEXT */)]), $setup.dialogFormVisible ? (_openBlock(), _createElementBlock(\"div\", _hoisted_16, [_createVNode($setup[\"CfDialog\"], {\n    modelValue: $setup.dialogFormVisible,\n    \"onUpdate:modelValue\": _cache[1] || (_cache[1] = $event => $setup.dialogFormVisible = $event),\n    onUpdateValue: $setup.handleUpdateValue,\n    ClassifyName: \"文章收藏\"\n  }, null, 8 /* PROPS */, [\"modelValue\"])])) : _createCommentVNode(\"v-if\", true), _createCommentVNode(\" 打赏盒子 \"), _createCommentVNode(\" <div class=\\\"icon-wrapper\\\" @click=\\\"emptyDialogVisible = true\\\">\\r\\n          <img class=\\\"like-pic\\\" :src=\\\"insertcoinsStatus === 0  ? uninsertcoinsImage : insertcoinsImage\\\">\\r\\n          <span class=\\\"like_num\\\">{{ articleData.rewardCount }}</span>\\r\\n        </div> \"), _createVNode(_component_el_dialog, {\n    class: \"reward\",\n    modelValue: $setup.emptyDialogVisible,\n    \"onUpdate:modelValue\": _cache[3] || (_cache[3] = $event => $setup.emptyDialogVisible = $event),\n    style: {\n      \"width\": \"310px\",\n      \"border-radius\": \"34px\",\n      \"overflow\": \"hidden\"\n    }\n  }, {\n    default: _withCtx(() => [_hoisted_17, _createElementVNode(\"div\", _hoisted_18, [_createElementVNode(\"div\", _hoisted_19, [_hoisted_20, _createVNode(_component_el_input, {\n      modelValue: $setup.input,\n      \"onUpdate:modelValue\": _cache[2] || (_cache[2] = $event => $setup.input = $event),\n      place