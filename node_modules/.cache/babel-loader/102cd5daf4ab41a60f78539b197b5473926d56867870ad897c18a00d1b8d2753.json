d;\r\n    showXXXXManagement.value = false;\r\n    showcomponent.value = true;\r\n  }\r\n}\r\n\r\nconst switchToComponent = (payload) => {\r\n  popboxInform.value = payload.title;\r\n  showXXXXManagement.value = true;\r\n  showcomponent.value = false;\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  background-color: #F5F5F5;\r\n  display: flex;\r\n  justify-content: center;\r\n  width: 86%;\r\n}\r\n\r\n.page {\r\n  background-color: #fff;\r\n  width: 92%;\r\n  padding: 2rem 3rem;\r\n}\r\n</style>"],"mappings":";;;EACOA,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC;AAAM;;uBADnBC,mBAAA,CAMM,OANNC,UAMM,GALJC,mBAAA,CAIM,OAJNC,UAIM,GAHkBC,MAAA,CAAAC,kBAAkB,I,cAAxCC,YAAA,CAC8CF,MAAA;;IADHG,2BAAyB,EAAEH,MAAA,CAAAI,yBAAyB;IAC5FC,YAAY,EAACL,MAAA,CAAAM;kFACCN,MAAA,CAAAO,aAAa,I,cAA9BL,YAAA,CAAiHM,wBAAA,CAA5ER,MAAA,CAAAS,eAAe;;IAAGC,mBAAiB,EAAEV,MAAA,CAAAW,iBAAiB;IAAGC,IAAI,EAACZ,MAAA,CAAAa,IAAI;IAAEC,EAAE,EAACd,MAAA,CAAAe"},"metadata":{},"sourceType":"module","externalDependencies":[]}